{
	"name": "cviebrock/image-validator",
	"description": "Custom Laravel 4 Validator for image dimensions",
	"keywords": ["laravel","validator","image"],
	"homepage": "https://github.com/cviebrock/image-validator",
	"license": "MIT",
	"authors": [
		{
			"name": "Colin Viebrock",
			"email": "colin@viebrock.ca"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"ext-gd": "*",
		"illuminate/support": ">=4.1.21",
		"illuminate/validation": ">=4.1.21"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"mockery/mockery": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"Cviebrock\\ImageValidator": "src/"
		}
	},
	"minimum-stability": "dev",
  "prefer-stable": true
}